{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "EFSEC2policy",
			"Effect": "Allow",
			"Action": [
				"elasticfilesystem:CreateMountTarget",
				"es:DescribeElasticsearchDomain",
				"es:DeleteElasticsearchDomain",
				"ec2:CreateSecurityGroup",
				"ec2:CreateTags",
				"ec2:DeleteSecurityGroup",
				"ec2:CreateVpc",
				"es:DescribeDomain",
				"es:DeleteDomain",
				"ec2:ModifyVpcAttribute",
				"elasticfilesystem:DescribeFileSystems",
				"ec2:CreateSubnet",
				"rds:CreateDBSubnetGroup",
				"rds:DescribDBSubnetGroup",
				"rds:DeleteDBSubnetGroup",
				"elasticfilesystem:DeleteFileSystem"
			],
			"Resource": [
				"arn:aws:rds:us-east-1:590183872451:subgrp:amv2-dbsubnetgroup",
				"arn:aws:elasticfilesystem:us-east-1:590183872451:file-system/fs-1234567890abcdef0",
				"arn:aws:ec2:*:590183872451:subnet/*",
				"arn:aws:ec2:*:590183872451:security-group/*",
				"arn:aws:ec2:*:590183872451:vpc/*",
				"arn:aws:es:us-east-1:590183872451:domain/my-opensearch-nw"
			]
		},
		{
			"Sid": "IAMpolicy",
			"Effect": "Allow",
			"Action": [
				"iam:GetRole",
				"elasticfilesystem:CreateFileSystem",
				"s3:*",
				"iam:TagRole",
				"iam:CreateRole",
				"iam:DeleteRole",
				"iam:DeleteRolePolicy",
				"es:CreateElasticsearchDomain",
				"iam:AttachRolePolicy",
				"iam:PutRolePolicy",
				"ec2:DescribeSecurityGroups",
				"elasticfilesystem:TagResource",
				"es:AddTags",
				"iam:GetServiceLinkedRoleDeletionStatus",
				"iam:DetachRolePolicy",
				"iam:CreateServiceLinkedRole",
				"elasticfilesystem:CreateMountTarget",
				"cloudformation:CreateStack",
				"ec2:DescribeAvailabilityZones",
				"ec2:DescribeVpcs",
				"iam:DeleteServiceLinkedRole",
				"ec2:DescribeSubnets",
				"elasticfilesystem:DescribeFileSystems"
			],
			"Resource": "*"
		}
	]
}